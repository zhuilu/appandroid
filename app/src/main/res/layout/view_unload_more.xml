<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="wrap_content"
    android:background="@color/white"
    android:id="@+id/unloadmoreRl"
    >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_20"
        android:layout_below="@+id/refreshIndexCell"
        android:layout_marginTop="@dimen/dp_20"
        android:layout_marginBottom="@dimen/dp_20"
        >

        <TextView
            android:id="@+id/footTv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="END"
            android:textColor="@color/_999999"
            android:textSize="@dimen/sp_11"
            android:layout_centerInParent="true"
            />


        <View
            android:layout_width="@dimen/dp_30"
            android:layout_height="@dimen/dp_0_2_5"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/footTv"
            android:background="@color/_e5e5e5"
            android:layout_marginRight="@dimen/dp_18"
            />
        <View
            android:layout_width="@dimen/dp_30"
            android:layout_height="@dimen/dp_0_2_5"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/footTv"
            android:background="@color/_e5e5e5"
            android:layout_marginLeft="@dimen/dp_18"
            />



    </RelativeLayout>
</RelativeLayout>