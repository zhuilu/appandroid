<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/dp_110" android:layout_height="@dimen/dp_127"
    android:background="@drawable/bg_companyhead"
    android:id="@+id/moreRl"
    android:layout_marginLeft="@dimen/dp_15"
    android:layout_marginRight="@dimen/dp_15"
    >

    <TextView
        android:id="@+id/moreTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="查看更多"
        android:layout_centerHorizontal="true"
        android:textColor="@color/colorPrimary"
        android:textSize="@dimen/sp_15"
        android:layout_marginTop="@dimen/dp_25"
        />
    <TextView
        android:id="@+id/mmember_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/moreTv"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/dp_12"
        android:textSize="@dimen/sp_12"
        android:textColor="@color/_999"
        />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/mmember_num"
        android:layout_centerHorizontal="true"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/dp_12"
        >
        
        <com.xinniu.android.qiqueqiao.widget.CircleImageView
            android:id="@+id/mmemberhead1"
            android:layout_width="@dimen/dp_25"
            android:layout_height="@dimen/dp_25"
            android:layout_marginLeft="@dimen/dp_1"
            android:layout_marginRight="@dimen/dp_1"
            />
        <com.xinniu.android.qiqueqiao.widget.CircleImageView
            android:id="@+id/mmemberhead2"
            android:layout_width="@dimen/dp_25"
            android:layout_height="@dimen/dp_25"
            android:layout_marginLeft="@dimen/dp_1"
            android:layout_marginRight="@dimen/dp_1"
            />
        <com.xinniu.android.qiqueqiao.widget.CircleImageView
            android:id="@+id/mmemberhead3"
            android:layout_width="@dimen/dp_25"
            android:layout_height="@dimen/dp_25"
            android:layout_marginLeft="@dimen/dp_1"
            android:layout_marginRight="@dimen/dp_1"
            />
        
        
        
        
    </LinearLayout>
    



    

</RelativeLayout>