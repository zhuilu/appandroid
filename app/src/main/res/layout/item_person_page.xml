<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/item_person_page_Ll"
    android:background="@color/white"
    >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/index_focus_cell"
        android:layout_centerInParent="true"
        android:layout_marginLeft="@dimen/dp_9"
        android:layout_marginRight="@dimen/dp_9"

        >



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"

        >


    <TextView
        android:id="@+id/item_my_resourcex_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        android:maxLines="2"
        android:lineSpacingExtra="@dimen/dp_5"
        android:letterSpacing="0.05"
        android:typeface="normal"
        android:textColor="@color/text_color_1"
        android:text="国家级举办国家,国家级举办国家级举办"
        android:textSize="@dimen/sp_16"
        android:paddingTop="@dimen/dp_2_5"
        android:paddingBottom="@dimen/dp_2_5"

        />

        <View
            android:id="@+id/line_view"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_0_2_5"
            android:background="@color/line_color"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginRight="@dimen/dp_15"
            android:layout_marginBottom="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_10"
            />


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_20"
        android:layout_marginBottom="@dimen/dp_8"
        >

        <RelativeLayout
            android:id="@+id/item_my_re_left"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/dp_20"
            >


            <ImageView
                android:id="@+id/resource_eyeImg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_10"
                android:src="@mipmap/eye_icon"
                android:layout_centerVertical="true"
                />
            <TextView
                android:id="@+id/resource_eyeTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/resource_eyeImg"
                android:layout_marginLeft="@dimen/dp_5"
                android:layout_centerVertical="true"
                android:text="1323"
                android:textSize="@dimen/sp_11"
                android:textColor="@color/text_gray_color2"
                />
            <ImageView
                android:id="@+id/resource_tellImg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_18"
                android:layout_toRightOf="@id/resource_eyeTv"
                android:src="@mipmap/tell_cion"
                android:layout_centerVertical="true"
                />
            <TextView
                android:id="@+id/resource_tellTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/resource_tellImg"
                android:layout_marginLeft="@dimen/dp_5"
                android:layout_centerVertical="true"
                android:text="131241"
                android:textSize="@dimen/sp_11"
                android:textColor="@color/text_gray_color2"
                />
        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/item_person_page"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_25"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_10"
            >

            <TextView
                android:id="@+id/item_index_recycler_mannertv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:layout_marginLeft="@dimen/dp_5"
                android:text="资源互换"
                android:textSize="@dimen/dp_11"
                android:background="@drawable/bg_text_cooptx"
                android:gravity="center"
                android:textColor="@color/colorPrimary"
                android:layout_centerVertical="true"

                />




        </RelativeLayout>







    </RelativeLayout>


    </LinearLayout>



    </RelativeLayout>




</RelativeLayout>