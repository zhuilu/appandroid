                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_16">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/dp_12"
                            android:text="剩余担保金"
                            android:textColor="@color/_333"
                            android:includeFontPadding="false"
                            android:textSize="@dimen/sp_14" />

                        <TextView
                            android:id="@+id/tv_price_s"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/dp_17"
                            android:text=""
                            android:includeFontPadding="false"
                            android:textColor="#ffee634e"
                            android:textSize="@dimen/sp_12" />
                    </RelativeLayout>
                    <TextView
                        android:id="@+id/tv_bj_status"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:textColor="#ff777777"
                        android:layout_marginRight="@dimen/dp_17"
                        android:textSize="@dimen/sp_12"
                        android:layout_marginTop="@dimen/dp_5"
                        android:visibility="gone"
                        android:layout_below="@+id/rlayout"/>

                    <TextView
                        android:id="@+id/tv_operation"
                        android:layout_width="@dimen/dp_72"
                        android:layout_height="@dimen/dp_26"
                        android:includeFontPadding="false"
                        android:layout_below="@+id/tv_bj_status"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginTop="@dimen/dp_11"
                        android:layout_marginRight="@dimen/dp_13"
                        android:layout_marginBottom="@dimen/dp_14"
                        android:background="@drawable/bg_trans_blue"
                        android:gravity="center"
                        android:text="补交担保金"
                        android:textColor="#ff4b96f3"
                        android:textSize="@dimen/sp_12"
                        android:visibility="visible" />

                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/rlayout_04"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_48"
                    android:visibility="gone">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/dp_12"
                        android:text="剩余担保金"
                        android:includeFontPadding="false"
                        android:textColor="@color/_333"
                        android:textSize="@dimen/sp_14" />


                    <TextView
                        android:id="@+id/tv_price_ss"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/dp_17"
