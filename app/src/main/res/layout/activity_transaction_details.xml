                                android:text="服务经理"
                                android:textColor="@color/_333"
                                android:textSize="@dimen/sp_14" />

                            <TextView
                                android:id="@+id/tv_identity"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="@dimen/dp_3"
                                android:background="@drawable/bg_trans_solid_organe"
                                android:gravity="center"
                                android:includeFontPadding="false"
                                android:paddingLeft="@dimen/dp_1"
                                android:paddingTop="@dimen/dp_1"
                                android:paddingRight="@dimen/dp_1"
                                android:paddingBottom="@dimen/dp_1"
                                android:text="卖方"
                                android:textColor="#ffffffff"
                                android:textSize="@dimen/sp_11" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/lx_positiontv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/ll"
                            android:layout_marginTop="@dimen/dp_6"
                            android:textColor="#ff777777"
                            android:textSize="@dimen/sp_12" />


                    </RelativeLayout>

                    <TextView
                        android:id="@+id/tv_chat"
                        android:layout_width="@dimen/dp_72"
                        android:layout_height="@dimen/dp_26"
                        android:includeFontPadding="false"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/dp_15"
                        android:background="@drawable/bg_trans_blue"
                        android:gravity="center"
                        android:text="在线联系"
                        android:textColor="#ff4b96f3"
                        android:textSize="@dimen/sp_12" />

                </RelativeLayout>

            </LinearLayout>


            <RelativeLayout
